					"script": {
						"exec": [
							"pm.test(\"get status response true\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"response must be valid and have a body\", function () {",
							"     pm.response.to.be.withBody;",
							"     pm.response.to.be.json;",
							"});",
							"",
							"pm.test(\"verify id returned is in numerical format\", () => {",
							"    const actualResponse = pm.response.json();",
							"",
							"    const isNumber = !isNaN(actualResponse.id)",
							"",
							"    pm.expect(isNumber).to.have.eql(true);",
