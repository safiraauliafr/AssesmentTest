		{
			"name": "Scenario 2 - Verify status for first entry is only either \"active\" or \"inactive\"",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.test(\"Verify status for first entry is only either 'active' or 'inactive'\", function () {",
							"     const payload = JSON.parse(pm.request.body.raw)",
							"     ",
							"     let status = payload.status",
							"",
							"     let isValid = true",
							"     // validate if status is number",
							"     if (!isNaN(status)){",
							"        isValid = false",
							"        pm.expect(isValid).to.have.eql(true)",
							"        return",
							"     }",
							"",
							"",
							"     // validate if status is NaN",
							"     if (isNaN(status)){",
							"        status = status.toLowerCase()",
							"     }",
							"",
							"     // validate if status is valid from hash map",
							"     const validStatus = new Map([[\"active\",true],[\"inactive\",true]])",
							"     if(!validStatus.get(status)){",
							"        isValid = false",
							"     }",
							"   ",
							"    pm.expect(isValid).to.have.eql(true)",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
